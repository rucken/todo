{
  "name": "rucken-todo",
  "version": "0.5.1",
  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "engines": {
    "node": ">=6",
    "npm": ">=5.3.0"
  },
  "private": true,
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "description": "Simple todo application with: projects, tasks, statuses (backend: http://mockapi.io).",
  "keywords": [
    "auth",
    "groups",
    "permissions",
    "projects",
    "tasks",
    "statuses",
    "rucken",
    "angular 5",
    "i18n",
    "ngx-bootstap",
    "bootstrap",
    "mockapi",
    "ssr"
  ],
  "homepage": "https://github.com/rucken/todo-web",
  "bugs": {
    "url": "https://github.com/rucken/todo-web/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rucken/todo-web.git"
  },
  "scripts": {
    "libs:build": "node ./node_modules/@rucken/cli/bin/rucken commands build ~~lib @rucken/todo-core link ~~lib @rucken/todo-core build ~~lib @rucken/todo-web",
    "libs:link": "node ./node_modules/@rucken/cli/bin/rucken commands link ~~lib",
    "libs:link-npm": "node ./node_modules/@rucken/cli/bin/rucken commands link-npm ~~lib",
    "app:serve-dev": "node ./node_modules/@angular/cli/bin/ng serve --app=todo --preserve-symlinks --aot",
    "app:serve-prod": "node ./node_modules/@angular/cli/bin/ng serve --app=todo --preserve-symlinks --aot --env=prod",
    "app:serve-mockapi": "node ./node_modules/@angular/cli/bin/ng serve --app=todo --preserve-symlinks --aot --env=mockapi",
    "app:start-dev": "npm-run-all clear prepare format libs:link-npm app:serve-dev",
    "app:start-prod": "npm-run-all clear prepare format libs:link-npm app:serve-prod",
    "app:start-mockapi": "npm-run-all clear prepare format libs:link-npm app:serve-mockapi",
    "app:start-full_dev": "npm-run-all clear prepare format libs:build libs:link app:serve-dev",
    "app:start-full_prod": "npm-run-all clear prepare format libs:build libs:link app:serve-prod",
    "app:start-full_mockapi": "npm-run-all clear prepare format libs:build libs:link app:serve-mockapi",
    "app:build-prod": "node ./node_modules/@angular/cli/bin/ng build --app=todo --preserve-symlinks --prod --build-optimizer --no-sourcemap",
    "app:build-full_prod": "npm-run-all clear prepare format libs:build libs:link app:build-prod",
    "app:build-mono_prod": "npm-run-all clear prepare format libs:link-npm app:build-prod",
    "app:build-mockapi": "node ./node_modules/@angular/cli/bin/ng build --app=todo --preserve-symlinks --prod --env=mockapi --build-optimizer --no-sourcemap",
    "app:build-mono_mockapi": "npm-run-all clear prepare format libs:link-npm app:build-mockapi",
    "app:e2e": "node ./node_modules/@angular/cli/bin/ng e2e --app=todo --preserve-symlinks",
    "lite-server": "lite-server -c bs-config.json",
    "lint": "node ./node_modules/@angular/cli/bin/ng lint",
    "format": "tsfmt -r",
    "clear": "node ./node_modules/@rucken/cli/bin/rucken clear",
    "prepare": "node ./node_modules/@rucken/cli/bin/rucken prepare",
    "test": "npm-run-all lint app:build-full_prod",
    "e2e": "node ./node_modules/@angular/cli/bin/ng e2e --app=todo --preserve-symlinks",
    "build": "npm-run-all app:build-full_prod",
    "start": "npm-run-all ssr:start",
    "gh:build-mockapi": "node ./node_modules/@angular/cli/bin/ng build --app=todo --preserve-symlinks --base-href https://rucken.github.io/todo-web/ --prod --env=mockapi --build-optimizer",
    "gh:build": "npm-run-all clear prepare format libs:build libs:link gh:build-mockapi",
    "gh:deploy": "cp-cli ./apps/todo/dist/browser/index.html ./apps/todo/dist/browser/404.html && gh-pages -d apps/todo/dist/browser",
    "ssr:build-server-mockapi": "node ./node_modules/@angular/cli/bin/ng build --app=todo-server --preserve-symlinks --prod --env=mockapi --output-hashing=false",
    "ssr:build-mockapi": "npm run ssr:client-and-server-bundles-mockapi && npm run ssr:webpack-server",
    "ssr:client-and-server-bundles-mockapi": "npm run app:build-mono_mockapi && npm run ssr:build-server-mockapi",
    "ssr:build-server-prod": "node ./node_modules/@angular/cli/bin/ng build --app=todo-server --preserve-symlinks --prod --env=prod --output-hashing=false",
    "ssr:build-prod": "npm run ssr:client-and-server-bundles-prod && npm run ssr:webpack-server",
    "ssr:client-and-server-bundles-prod": "npm run app:build-mono_prod && npm run ssr:build-server-prod",
    "ssr:webpack-server": "webpack --config webpack.server.config.js --progress --colors",
    "ssr:start": "node ./server.js",
    "heroku-postbuild": "npm-run-all ssr:build-mockapi"
  },
  "cacheDirectories": [],
  "dependencies": {
    "@angular/animations": "5.2.2",
    "@angular/common": "5.2.2",
    "@angular/compiler": "5.2.2",
    "@angular/core": "5.2.2",
    "@angular/forms": "5.2.2",
    "@angular/http": "5.2.2",
    "@angular/platform-browser": "5.2.2",
    "@angular/platform-browser-dynamic": "5.2.2",
    "@angular/platform-server": "^5.2.2",
    "@angular/platform-webworker": "5.2.2",
    "@angular/platform-webworker-dynamic": "5.2.2",
    "@angular/router": "5.2.2",
    "@nguniversal/express-engine": "^5.0.0-beta.5",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
    "@ngx-translate/core": "^9.1.1",
    "@rucken/core": "^1.30.5",
    "@rucken/web": "^1.30.5",
    "@types/express": "^4.11.0",
    "@types/jasmine": "2.8.6",
    "@types/lodash": "^4.14.97",
    "@types/marked": "0.3.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^9.4.0",
    "@types/webpack": "^3.8.4",
    "angular2-text-mask": "^8.0.4",
    "core-js": "^2.5.3",
    "lodash": "^4.17.4",
    "ngx-bootstrap": "^2.0.2",
    "rxjs": "^5.5.6",
    "text-mask-addons": "^3.7.1",
    "ts-loader": "^3.3.1",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "1.6.6",
    "@angular/compiler-cli": "^5.2.2",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^4.1.0",
    "jasmine-core": "~2.9.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",
    "typescript-formatter": "^7.0.1",
    "@rucken/cli": "1.8.0",
    "npm-run-all": "^4.1.2",
    "conventional-changelog-cli": "^1.3.8",
    "conventional-commits-detector": "^0.1.1",
    "conventional-recommended-bump": "^2.0.0",
    "gh-pages": "^1.1.0",
    "html-loader": "0.5.5",
    "lite-server": "^2.3.0",
    "markdown-loader": "^2.0.2",
    "cp-cli": "^1.1.0"
  },
	"greenkeeper": {
	  "ignore": [
		"typescript"
	  ]
	}
}
