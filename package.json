{
  "name": "rucken-todo",
  "version": "0.3.0",
  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "description": "Simple todo application with: projects, tasks, statuses (backend: http://mockapi.io).",
  "keywords": [
    "auth",
    "groups",
    "permissions",
    "projects",
    "tasks",
    "statuses",
    "rucken",
    "angular 5",
    "i18n",
    "ngx-bootstap",
    "bootstrap",
    "mockapi"
  ],
  "homepage": "https://github.com/rucken/todo-web",
  "bugs": {
    "url": "https://github.com/rucken/todo-web/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rucken/todo-web.git"
  },
  "scripts": {
    "lib-core:clear": "./node_modules/.bin/del-cli ./libs/core/src/node_modules ./libs/core/dist ./libs/core/.tmp",
    "lib-core:link": "ngm link -p ./libs/core/src --here",
    "lib-core:build": "ngm build -p ./libs/core/src --clean",
    "lib-core:link-npm": "npm link ./libs/core/src",
    "lib-core:link-npm-clear": "./node_modules/.bin/del-cli ./libs/core/src/node_modules ./libs/core/dist/node_modules",
    "lib-core:tools-change_version": "gulp lib-core:change-version",
    "lib-core:tools-extract_translate": "ngx-translate-extract --input ./libs/core/src --output ./libs/core/src/i18n/template.pot --format=pot --marker translate --clean",
    "lib-core:tools-po2ts": "./node_modules/.bin/srcgen -x -t convert.po.to.ts -f ./srcgen/core-convert.po.to.ts.json",
    "lib-core:tools-make_ts_list": "./node_modules/.bin/srcgen -x -t make.list.ts.files -f ./srcgen/core-make.list.ts.files.json",
    "lib-core:tools-prepare": "npm-run-all lib-core:tools-extract_translate lib-core:tools-po2ts lib-core:tools-make_ts_list",
    "lib-core:pack": "npm pack ./libs/core/dist",
    "lib-web:clear": "./node_modules/.bin/del-cli ./libs/web/src/node_modules ./libs/web/dist ./libs/web/.tmp",
    "lib-web:link": "ngm link -p ./libs/web/src --here",
    "lib-web:build": "ngm build -p ./libs/web/src --clean",
    "lib-web:link-npm": "npm link ./libs/web/src",
    "lib-web:link-npm-clear": "./node_modules/.bin/del-cli ./libs/web/src/node_modules ./libs/web/dist/node_modules",
    "lib-web:tools-change_version": "gulp lib-web:change-version",
    "lib-web:tools-po2ts": "./node_modules/.bin/srcgen -x -t convert.po.to.ts -f ./srcgen/web-convert.po.to.ts.json",
    "lib-web:tools-make_ts_list": "./node_modules/.bin/srcgen -x -t make.list.ts.files -f ./srcgen/web-make.list.ts.files.json",
    "lib-web:tools-extract_translate": "ngx-translate-extract --input ./libs/web/src --output ./libs/web/src/i18n/template.pot --format=pot --marker translate --clean",
    "lib-web:tools-prepare": "npm-run-all lib-web:tools-extract_translate lib-web:tools-po2ts lib-web:tools-make_ts_list",
    "lib-web:pack": "npm pack ./libs/web/dist",
    "libs:clear": "npm-run-all lib-core:clear lib-web:clear",
    "libs:change_version": "npm-run-all lib-core:tools-change_version lib-web:tools-change_version",
    "libs:prepare": "npm-run-all lib-core:tools-prepare lib-web:tools-prepare",
    "libs:pack": "npm-run-all lib-core:pack lib-web:pack",
    "libs:build": "npm-run-all libs:change_version lib-core:build lib-core:link lib-web:build lib-core:pack lib-web:pack",
    "libs:link": "npm-run-all lib-core:link lib-web:link lib-core:link-npm-clear lib-web:link-npm-clear",
    "libs:link-npm": "npm-run-all lib-core:link-npm lib-web:link-npm lib-core:link-npm-clear lib-web:link-npm-clear",
    "app-todo:github_pages": "npm-run-all app-todo:github_full_build app-todo:github_deploy",
    "app-todo:clear": "./node_modules/.bin/del-cli ./apps/todo/dist",
    "app-todo:github_build": "ng build --app=todo  --preserve-symlinks --base-href https://rucken.github.io/todo-web/ --prod --aot --env=mockapi --build-optimizer",
    "app-todo:github_full_build": "npm-run-all libs:build libs:link app-todo:github_build app-todo:tools-add_version",
    "app-todo:github_deploy": "gh-pages -d apps/todo/dist",
    "app-todo:lite_server": "lite-server -c bs-config.json",
    "app-todo:serve_dev": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng serve --app=todo  --preserve-symlinks --host 0.0.0.0 --aot=false --env=mockapi",
    "app-todo:serve_prod": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng serve --app=todo  --preserve-symlinks --host 0.0.0.0 --aot",
    "app-todo:serve_mockapi": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng serve --app=todo  --preserve-symlinks --host 0.0.0.0 --aot --env=mockapi",
    "app-todo:start-full_dev": "npm-run-all libs:prepare libs:build libs:link app-todo:serve_dev",
    "app-todo:start-full_prod": "npm-run-all libs:prepare libs:build libs:link app-todo:serve_prod",
    "app-todo:start-full_mockapi": "npm-run-all libs:prepare libs:build libs:link app-todo:serve_mockapi",
    "app-todo:prod_build": "ng build --app=todo  --preserve-symlinks --base-href / --prod --aot --build-optimizer",
    "app-todo:prod_full_build": "npm-run-all libs:build libs:link app-todo:prod_build app-todo:tools-add_version",
    "app-todo:tools-add_version": "gulp app-todo:add-version",
    "app-todo:start-dev": "npm-run-all libs:prepare libs:link-npm app-todo:serve_dev",
    "app-todo:start-prod": "npm-run-all libs:prepare libs:link-npm app-todo:serve_prod",
    "app-todo:start-mockapi": "npm-run-all libs:prepare libs:link-npm app-todo:serve_mockapi",
    "app-todo:tools-extract_translate": "ngx-translate-extract --input ./apps/todo/src/app --output ./apps/todo/src/app/i18n/template.pot --format=pot --marker translate --clean",
    "app-todo:tools-po2ts": "./node_modules/.bin/srcgen -x -t convert.po.to.ts -f ./srcgen/todo-convert.po.to.ts.json",
    "app-todo:tools-make_ts_list": "./node_modules/.bin/srcgen -x -t make.list.ts.files -f ./srcgen/todo-make.list.ts.files.json",
    "app-todo:tools-prepare": "npm-run-all app-todo:tools-extract_translate app-todo:tools-po2ts app-todo:tools-make_ts_list",
    "app-todo:lint": "ng lint --app=todo  --preserve-symlinks",
    "app-todo:e2e": "ng e2e --app=todo  --preserve-symlinks",
    "apps:clear": "npm-run-all app-todo:clear",
    "apps:prepare": "npm-run-all app-todo:tools-prepare",
    "all:clear": "npm-run-all libs:clear apps:clear",
    "all:prepare": "npm-run-all libs:prepare apps:prepare",
    "gh:build": "npm-run-all app-todo:github_full_build",
    "gh:deploy": "npm-run-all app-todo:github_deploy",
    "ng": "ng",
    "test": "npm-run-all app-todo:lint app-todo:prod_full_build",
    "build": "npm-run-all app-todo:prod_full_build",
    "start": "npm-run-all app-todo:start-mockapi",
    "make:grid_run": "srcgen -t grid.select.input.modal -f ./srcgen/grid.select.input.modal.json",
    "make:grid": "npm-run-all make-grid_run libs:prepare"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.2",
    "@angular/common": "^5.0.2",
    "@angular/compiler": "^5.0.2",
