{
  "name": "rucken-todo",
  "version": "0.3.1",
  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "description": "Simple todo application with: projects, tasks, statuses (backend: http://mockapi.io).",
  "keywords": [
    "auth",
    "groups",
    "permissions",
    "projects",
    "tasks",
    "statuses",
    "rucken",
    "angular 5",
    "i18n",
    "ngx-bootstap",
    "bootstrap",
    "mockapi"
  ],
  "homepage": "https://github.com/rucken/todo-web",
  "bugs": {
    "url": "https://github.com/rucken/todo-web/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rucken/todo-web.git"
  },
  "scripts": {
    "libs:build": "node ./node_modules/@rucken/cli/bin/rucken commands build ~~lib @rucken/todo-core link ~~lib @rucken/todo-core build ~~lib @rucken/todo-web",
    "libs:link": "node ./node_modules/@rucken/cli/bin/rucken commands link ~~lib",
    "libs:link-npm": "node ./node_modules/@rucken/cli/bin/rucken commands link-npm ~~lib",
    "app:serve-dev": "node ./node_modules/@angular/cli/bin/ng serve --app=todo --preserve-symlinks --aot=false",
    "app:serve-prod": "node ./node_modules/@angular/cli/bin/ng serve --app=todo --preserve-symlinks --aot",
    "app:serve-mockapi": "node ./node_modules/@angular/cli/bin/ng serve --app=todo --preserve-symlinks --aot --env=mockapi",
    "app:start-dev": "npm-run-all prepare format libs:link-npm app:serve-dev",
    "app:start-prod": "npm-run-all prepare format libs:link-npm app:serve-prod",
    "app:start-mockapi": "npm-run-all prepare format libs:link-npm app:serve-mockapi",
    "app:start-full_dev": "npm-run-all prepare format libs:build libs:link app:serve-dev",
    "app:start-full_prod": "npm-run-all prepare format libs:build libs:link app:serve-prod",
    "app:start-full_mockapi": "npm-run-all prepare format libs:build libs:link app:serve-mockapi",
    "app:build-prod": "node ./node_modules/@angular/cli/bin/ng build --app=todo --preserve-symlinks --prod --build-optimizer",
    "app:build-full_prod": "npm-run-all prepare format libs:build libs:link app:build-prod",
    "app:build-github": "node ./node_modules/@angular/cli/bin/ng build --app=todo --preserve-symlinks --base-href https://rucken.github.io/todo-web/ --prod --env=mockapi --build-optimizer",
    "app:build-full_github": "npm-run-all prepare format libs:build libs:link app:build-github",
    "app:deploy-github": "gh-pages -d apps/todo/dist",
    "gh:build": "npm-run-all app:build-full_github",
    "gh:deploy": "npm-run-all app:deploy-github",
    "lite-server": "lite-server -c bs-config.json",
    "ng": "ng",
    "lint": "ng lint",
    "format": "tsfmt -r",
    "prepare": "node ./node_modules/@rucken/cli/bin/rucken commands clear prepare",
    "test": "npm-run-all lint app:build-full_prod",
    "e2e": "ng e2e --app=todo --preserve-symlinks",
    "build": "npm-run-all app:build-full_prod",
    "deploy": "npm-run-all app:deploy-github",
    "start": "npm-run-all app:start-mockapi"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.1.0",
    "@angular/common": "^5.1.0",
    "@angular/compiler": "^5.1.0",
    "@angular/core": "^5.1.0",
    "@angular/forms": "^5.1.0",
    "@angular/http": "^5.1.0",
    "@angular/platform-browser": "^5.1.0",
    "@angular/platform-browser-dynamic": "^5.1.0",
    "@angular/router": "^5.1.0",
    "@ngui/auto-complete": "^0.16.0",
    "@ngx-translate/core": "^9.0.1",
    "@rucken/core": "^1.28.2",
    "@rucken/web": "^1.28.2",
    "@types/jasmine": "2.8.2",
    "@types/lodash": "^4.14.88",
    "@types/marked": "0.3.0",
    "@types/node": "^8.0.56",
    "@types/requirejs": "^2.1.31",
    "@types/webpack": "^3.8.1",
    "angular2-ladda": "^1.2.2",
    "angular2-text-mask": "^8.0.4",
    "awesome-typescript-loader": "^3.4.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.1",
    "intl": "^1.2.5",
    "ladda": "^1.0.5",
    "lodash": "^4.17.4",
    "ngx-bootstrap": "^2.0.0-beta.10",
    "rxjs": "^5.5.5",
    "text-mask-addons": "^3.7.1",
    "tslib": "^1.8.1",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.0",
    "@angular/compiler-cli": "^5.1.0",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@rucken/cli": "^1.4.1",
    "codelyzer": "~4.0.1",
    "conventional-changelog-cli": "^1.3.5",
    "conventional-commits-detector": "^0.1.1",
    "conventional-recommended-bump": "^1.0.3",
    "es6-promise-loader": "^1.0.2",
    "gh-pages": "^1.1.0",
    "html-loader": "0.5.1",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.3.0",
    "locate-path": "^2.0.0",
    "markdown-loader": "^2.0.1",
    "ngm-cli": "^0.8.3",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "protractor": "~5.2.1",
    "shebang-command": "^1.2.0",
    "ts-node": "~4.0.0",
    "tslint": "~5.8.0",
    "typescript": "2.5.3",
    "typescript-formatter": "^7.0.0",
    "typings": "^2.1.1"
  }
}
