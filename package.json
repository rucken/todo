{
  "name": "rucken-todo",
  "version": "0.1.3",
  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "description": "Simple todo application with: projects, tasks, statuses (backend: http://mockapi.io).",
  "keywords": [
    "auth",
    "groups",
    "permissions",
    "projects",
    "tasks",
    "statuses",
    "rucken",
    "angular 4",
    "i18n",
    "ngx-bootstap",
    "bootstrap",
    "mockapi"
  ],
  "homepage": "https://github.com/site15/rucken-todo",
  "bugs": {
    "url": "https://github.com/site15/rucken-todo/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/site15/rucken-todo.git"
  },
  "scripts": {
    "start": "ng serve",
    "test": "npm-run-all lint build",
    "lint": "ng lint",
    "app.build": "ng build --base-href https://site15.github.io/rucken-todo/ --prod --aot=false",
    "app.add-version": "gulp add-version",
    "app.deploy": "gh-pages -d dist",
    "app.gh-pages": "npm-run-all app.build app.add-version app.deploy",
    "build": "npm-run-all app.build app.add-version",
    "_extract_translate": "ngx-translate-extract --input ./src/app/todo --output ./src/app/todo/i18n/template.pot --format=pot --marker translate --clean",
    "_po2ts": "srcgen -x -t convert.po.to.ts -f ./srcgen/convert.po.to.ts.json",
    "_make_ts_list": "srcgen -x -t make.list.ts.files -f ./srcgen/make.list.ts.files.json",
    "_prepare": "npm-run-all _extract_translate _po2ts _make_ts_list",
    "_grid.run": "srcgen -t grid.select.input.modal -f ./srcgen/grid.select.input.modal.json",
    "_grid": "npm-run-all _grid.run _prepare"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.2.5",
    "@angular/compiler": "^4.2.5",
    "@angular/core": "^4.2.5",
    "@angular/forms": "^4.2.5",
    "@angular/http": "^4.2.5",
    "@angular/platform-browser": "^4.2.5",
    "@angular/platform-browser-dynamic": "^4.2.5",
    "@angular/router": "^4.2.5",
    "core-js": "^2.4.1",
    "rucken": "^1.10.4",
    "rxjs": "^5.4.1",
    "zone.js": "^0.8.12",
    "@ngui/auto-complete": "^0.13.3",
    "@ngx-translate/core": "^7.0.0",
    "@types/lodash": "^4.14.68",
    "angular2-jwt": "^0.2.3",
    "angular2-text-mask": "^8.0.2",
    "lodash": "^4.17.4",
    "ngx-bootstrap": "^1.7.1",
    "text-mask-addons": "^3.6.0",
    "tslib": "^1.7.1",
    "angular2-ladda": "^1.2.1"
  },
  "devDependencies": {
    "@angular/cli": "^1.2.0",
    "@angular/compiler-cli": "^4.2.5",
    "@biesbjerg/ngx-translate-extract": "^2.3.1",
    "@types/jasmine": "2.5.53",
    "@types/node": "~8.0.7",
    "angular2-template-loader": "^0.6.2",
    "codelyzer": "~3.1.1",
    "conventional-changelog-cli": "^1.3.1",
    "conventional-commits-detector": "^0.1.1",
    "conventional-recommended-bump": "^1.0.0",
    "gh-pages": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-inline-ng2-template": "^4.0.0",
    "gulp-replace": "^0.6.1",
    "gulp-tsc": "^1.3.2",
    "html-loader": "0.4.5",
    "jasmine-core": "~2.6.4",
    "jasmine-spec-reporter": "~4.1.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lodash": "^4.17.4",
    "markdown-loader": "github:valorkin/markdown-loader",
    "npm-run-all": "^4.0.2",
    "po2json": "^0.4.5",
    "protractor": "~5.1.2",
    "recursive-readdir": "^2.2.1",
    "replace-ext": "^1.0.0",
    "srcgen": "^0.2.13",
    "ts-node": "~3.2.1",
    "tslint": "~5.4.3",
    "typescript": "~2.4.1"
  }
}
