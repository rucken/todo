{
  "name": "rucken-todo",
  "version": "0.1.6",
  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "description": "Simple todo application with: projects, tasks, statuses (backend: http://mockapi.io).",
  "keywords": [
    "auth",
    "groups",
    "permissions",
    "projects",
    "tasks",
    "statuses",
    "rucken",
    "angular 4",
    "i18n",
    "ngx-bootstap",
    "bootstrap",
    "mockapi"
  ],
  "homepage": "https://github.com/site15/rucken-todo",
  "bugs": {
    "url": "https://github.com/site15/rucken-todo/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/site15/rucken-todo.git"
  },
  "scripts": {
    "start": "npm-run-all tools-prepare app-serve",
    "demo": "npm-run-all tools-prepare demo-serve",
    "test": "npm-run-all lint build",
    "lint": "ng lint",
    "build": "npm-run-all app-build tools-add_version",
    "app-build": "ng build --prod --aot",
    "app-serve": "ng serve --host 0.0.0.0 --aot",
    "demo-serve": "ng serve --host 0.0.0.0 --aot --env=mockapi",
    "demo-prod_build": "ng build --base-href https://site15.github.io/rucken-todo/ --prod --env=mockapi --aot --build-optimizer",
    "demo-build": "npm-run-all demo-prod_build tools-demo-add_version",
    "demo-deploy": "gh-pages -d dist",
    "demo-gh_pages": "npm-run-all demo-build demo-deploy",
    "tools-demo-add_version": "gulp add-version",
    "tools-build": "npm-run-all demo-build tools-demo-add_version",
    "tools-extract_translate": "ngx-translate-extract --input ./src/app/todo --output ./src/app/todo/i18n/template.pot --format=pot --marker translate --clean",
    "tools-po2ts": "srcgen -x -t convert.po.to.ts -f ./srcgen/convert.po.to.ts.json",
    "tools-make_ts_list": "srcgen -x -t make.list.ts.files -f ./srcgen/make.list.ts.files.json",
    "tools-prepare": "npm-run-all tools-extract_translate tools-po2ts tools-make_ts_list",
    "make-grid_run": "srcgen -t grid.select.input.modal -f ./srcgen/grid.select.input.modal.json",
    "make-grid": "npm-run-all make-grid_run tools-prepare"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.4.2",
    "@angular/common": "^4.4.2",
    "@angular/compiler": "^4.4.2",
    "@angular/core": "^4.4.2",
    "@angular/forms": "^4.4.2",
    "@angular/http": "^4.4.2",
    "@angular/platform-browser": "^4.4.2",
    "@angular/platform-browser-dynamic": "^4.4.2",
    "@angular/router": "^4.4.2",
    "@ngui/auto-complete": "^0.14.4",
    "@ngx-translate/core": "^8.0.0",
    "@types/jasmine": "2.6.0",
    "@types/lodash": "^4.14.74",
    "@types/marked": "0.3.0",
    "@types/node": "^8.0.28",
    "@types/requirejs": "^2.1.31",
    "@types/webpack": "^3.0.10",
    "angular2-jwt": "^0.2.3",
    "angular2-ladda": "^1.2.1",
    "angular2-text-mask": "^8.0.4",
    "awesome-typescript-loader": "^3.2.3",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.1",
    "intl": "^1.2.5",
    "lodash": "^4.17.4",
    "ngx-bootstrap": "^1.9.3",
    "rucken": "^1.13.2",
    "rxjs": "^5.4.3",
    "text-mask-addons": "^3.6.0",
    "tslib": "^1.7.1",
    "zone.js": "^0.8.17"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.2",
    "@angular/compiler-cli": "^4.4.2",
    "@biesbjerg/ngx-translate-extract": "^2.3.2",
    "codelyzer": "~3.2.0",
    "conventional-changelog-cli": "^1.3.3",
    "conventional-commits-detector": "^0.1.1",
    "conventional-recommended-bump": "^1.0.1",
    "es6-promise-loader": "^1.0.2",
    "gh-pages": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-replace": "^0.6.1",
    "gulp-tsc": "^1.3.2",
    "html-loader": "0.5.1",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.3.0",
    "locate-path": "^2.0.0",
    "markdown-loader": "^2.0.1",
    "ngm-cli": "^0.7.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "po2json": "^0.4.5",
    "protractor": "~5.1.2",
    "recursive-readdir": "^2.2.1",
    "replace-ext": "^1.0.0",
    "shebang-command": "^1.2.0",
    "srcgen": "^0.3.0",
    "ts-node": "~3.3.0",
